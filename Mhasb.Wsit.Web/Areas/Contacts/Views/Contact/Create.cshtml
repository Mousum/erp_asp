@model Mhasb.Wsit.CustomModel.Contact.ContactCustome



<div class="container my-dashboard">
    <div class="row clearfix no-margin">
        <div class="col-md-12 no-padding">
            @if (TempData["SucMasg"] != null)
            {
                <div class="alert alert-success">@TempData["SucMasg"]</div>
            }
            @if (TempData["errMsg"] != null)
            {
                <div class="alert alert-danger">@TempData["errMsg"]</div>
            }
            <h2 class="txt-bold promo-text"><span class="promo-highlight">Join</span> to the MHASB Community for FREE !!</h2>
            <p class="promo-p text-center">
                Lorem ipsum dolor sit amet, consectetur adipiscing elit. Phasellus vehicula nunc blandit eros facilisis malesuada. Integer in orci turpis. Praesent id risus ullamcorper, iaculis est eget, scelerisque nulla. Integer magna risus
            </p>
        </div>
    </div>
    <div class="row clearfix no-margin">
        <h3 class="all-head">Add Contact</h3>
        <div class="col-md-6 clearfix pull-right text-right">
            <label class="text-uppercase">List My profile:</label>
            <input type="checkbox" checked data-toggle="toggle" data-onstyle="success" data-width="100" data-size="small">
        </div>
    </div>
    <div class="row clearfix p-setting-container">
        <div class="col-md-8 base-block no-margin clearfix">

            <h4 class="all-head">Contact Details</h4>
            <div class="row cstm-frm-row">
                <div class="col-md-12 pad-right-null">
                    <div class="col-md-12"><label id="errmsg"></label></div>
                    <label class="btn-block-dis">Contact Name:</label>
                    <div class="col-md-6 no-padding">
                        @Html.TextBoxFor(model => model.ContactInformation.ContactName, new { @class = "cstm-input", @placeholder = "Contact Name" })
                        @Html.ValidationMessageFor(model => model.ContactInformation.ContactName)
                    </div>

                </div>
            </div>

        </div>
    </div>
    <div class="col-md-6">
        <input type="submit" class="btn btn-success btn-block-dis pull-right" value="Save">
    </div>
</div>








